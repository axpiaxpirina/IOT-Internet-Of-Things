/* 
 * Led externo
 * IoT Internet Of Things
 * 2021
*/

void setup() {
  pinMode(LED_BUILTIN, OUTPUT);
  pinMode(D5, OUTPUT);
  Serial.begin(9600);
}

void loop() {
  Serial.println("LEDs encendido");
  digitalWrite(LED_BUILTIN, HIGH);
  digitalWrite(D5, HIGH);
  delay(5000);

  Serial.println("LEDs apagado");
  digitalWrite(LED_BUILTIN, LOW);
  digitalWrite(D5, LOW);
  delay(5000);
}
